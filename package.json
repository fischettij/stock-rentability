{
  "name": "stock-rentability",
  "version": "1.0.0",
  "description": "Stack rentability backend",
  "main": "server.js",
  "author": "Fische",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --ignore node_modules ./index.js",
    "start": "node ./index.js",
    "lint": "eslint --ext .js ./ src/ test/",
    "test": "jest --verbose",
    "test:ci": "jest --verbose --coverage",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-routes-catalogue": "^1.1.0",
    "express-validator": "^6.5.0",
    "http-status-codes": "^1.4.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "sequelize": "^5.21.3",
    "sqlite3": "^4.1.1",
    "winston-ready": "^2.0.1"
  },
  "devDependencies": {
    "coveralls": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "jest": "^25.4.0",
    "jest-extended": "^0.11.5",
    "nodemon": "^2.0.2",
    "sequelize-cli": "^5.5.1",
    "supertest": "^4.0.2"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "clover",
      "html"
    ],
    "collectCoverageFrom": [
      "src/*.js",
      "src/**/*.js"
    ]
  }
}
